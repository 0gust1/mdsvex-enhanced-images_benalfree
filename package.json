{
  "name": "mdsvex-enhanced-images",
  "description": "An MDsveX plugin to preprocess Markdown images using @sveltejs/enhanced-img.",
  "author": {
    "name": "benallfree",
    "url": "https://github.com/benallfree"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benallfree/mdsvex-enhanced-images/issues"
  },
  "homepage": "https://github.com/benallfree/mdsvex-enhanced-images",
  "repository": {
    "type": "git",
    "url": "https://github.com/benallfree/mdsvex-enhanced-images.git"
  },
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup-node src/index.ts --format=esm  --dts",
    "watch": "tsup-node src/index.ts --format=esm  --dts --watch"
  },
  "peerDependencies": {},
  "dependencies": {
    "just-camel-case": "^6.2.0",
    "unist-util-visit": "^5.0.0"
  },
  "files": [
    "dist",
    "*.md"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.7.5",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsup": "^8.3.0",
    "typescript": "^5.0.0",
    "mdast": "^3.0.0",
    "unified": "^11.0.5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  }
}
